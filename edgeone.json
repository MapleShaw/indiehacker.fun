{
  "headers": [
        {
        "source": "/images/*.jpg",
        "headers": [
            {
                "key": "Cache-Control",
                "value": "public, max-age=31536000, immutable"
            }
        ]
      }
  ],
  "caches": [
    {
      "source": "/images/*",
      "cacheTtl": 86400
    }
  ],
  "redirects": [
    {
      "source": "/about",
      "destination": "/contact",
      "statusCode": 301
    },
    {
      "source": "/price",
      "destination": "https://github.com/TencentEdgeOne/pages-templates/tree/main/examples/chrome-ai",
      "statusCode": 301
    }
  ]
}
