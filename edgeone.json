{
  "buildCommand": "pnpm run build",
  "outputDirectory": "./build",
  "headers": [
        {
        "source": "/images/*.jpg",
        "headers": [
            {
                "key": "Cache-Control",
                "value": "public, max-age=31536000, immutable"
            }
        ]
      }
  ],
  "redirects": [
    {
      "source": "/about",
      "destination": "https://www.baidu.com",
      "statusCode": 301
    },
    {
      "source": "/contact",
      "destination": "https://www.qq.com",
      "statusCode": 302
    }
  ]
}